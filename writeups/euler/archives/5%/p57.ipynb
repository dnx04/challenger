   "source": [
    "def p57(n):\n",
    "    seq = continued_fraction(sqrt(2))\n",
    "    return len([i for i in range(1, n + 1) if len(str(seq.convergent(i).numer())) > len(str(seq.convergent(i).denom()))])"
   ]
