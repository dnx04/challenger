   "metadata": {},
   "outputs": [],
   "source": [
    "def p48(n):\n",
    "    MOD = 10^10\n",
    "    R = IntegerModRing(MOD)\n",
    "    return sum(R(i)^i for i in range(1, n + 1)) % MOD"
   ]
  },
